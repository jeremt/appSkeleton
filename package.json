{
  "name": "appSkeleton",
  "version": "0.1.1",
  "description": "Simple web application skeleton.",
  "repository": {
    "type": "git",
    "url": "git://github.com/jeremt/appSkeleton.git"
  },
  "browser": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify-shim": "~3.4.1",
    "browserify": "~3.36.0",
    "coffeeify": "~0.6.0",
    "connect": "~2.14.3",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp": "~3.6.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "gulp-util": "~2.2.14",
    "pretty-hrtime": "~0.2.1",
    "coffee-script": "^1.7.1",
    "gulp-jade": "^0.6.1",
    "gulp-stylus": "^1.1.0"
  }
}
